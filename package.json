{
  "name": "pia",
  "version": "v2.3.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "cypress open --config watchForFileChanges=false",
    "prod": "ng build --prod --build-optimizer",
    "clean": "rimraf ./dist/",
    "format:write": "prettier src/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier src/**/*.{ts,json,md,scss} --list-different",
    "extract-i18n": "ngx-translate-extract --input ./src --output ./src/assets/i18n/strings.json --clean --sort --format namespaced-json",
    "pwa:start-server": "cd dist/pia & http-server -o",
    "pwa:create-pwa-icons": "ngx-pwa-icons",
    "compodoc": "compodoc -p src/tsconfig.app.json"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not IE 9-11"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.0",
    "@angular/common": "~11.2.0",
    "@angular/compiler": "~11.2.0",
    "@angular/core": "~11.2.0",
    "@angular/forms": "~11.2.0",
    "@angular/platform-browser": "~11.2.0",
    "@angular/platform-browser-dynamic": "~11.2.0",
    "@angular/router": "~11.2.0",
    "@angular/service-worker": "~11.2.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "angular2-csv": "git+https://github.com/kosmas58/angular2-csv.git",
    "core-js": "^3.8.3",
    "d3": "^6.5.0",
    "file-saver": "^2.0.5",
    "font-awesome": "^4.7.0",
    "foundation-sites": "^6.6.3",
    "html2canvas": "1.0.0-rc.1",
    "intro.js": "^3.3.1",
    "jszip": "^3.6.0-0",
    "ng2-pdfjs-viewer": "^5.0.7",
    "ngx-chips": "2.2.2",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.6.3",
    "rxjs-compat": "^6.6.3",
    "save-svg-as-png": "^1.4.17",
    "tinymce": "^4.9.11",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.4",
    "@angular/cli": "~11.1.4",
    "@angular/compiler-cli": "~11.2.0",
    "@angular/language-service": "~11.2.0",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/intro.js": "~3.0.0",
    "@types/jasmine": "~3.6.3",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~14.14.25",
    "codelyzer": "^6.0.1",
    "cypress": "^6.4.0",
    "http-server": "^0.12.3",
    "husky": "^5.0.9",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ngx-pwa-icons": "^0.1.2",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.0.5"
  }
}
